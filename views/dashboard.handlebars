<div class="flex">
    {{> navbar }}
    <div class="Container">
        {{#if messages.success}}
            <div class="msg successMsg">
                {{messages.success}}
            </div>
        {{/if}}        
        {{#if messages.fail}}
            <div class="msg failMsg">
                {{messages.fail}}
            </div>
        {{/if}}
        <h1>Dashboard</h1>
        
    </div>
</div>