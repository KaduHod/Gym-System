<div class="center">
    <h1>Registre-se</h1>
    <form action="/register" id="form" class="flexClolumn" method="POST">
        
        <input name="nome" placeholder="Digite a sua nome" type="text" />

        <input name="cpf" placeholder="Digite o seu cpf" type="text" />

        <input name="telefone" placeholder="Digite o seu telefone" onkeypress="mask(this, mphone);" o type="text" />

        <input name="dataNascimento" placeholder="Data de nascimento" type="date" />

        <input name="email" required placeholder="Digite o seu email" type="email" />
        
        <input name="senha" required placeholder="Digite a sua senha" type="password" />
        
        <input type="submit" value="Registre-se" />
    </form>
    
</div>
<script src="/js/handleFormRegister.js"></script>
<script>
  const form = document.getElementById('form')
  form.addEventListener('submit', handleRegisterProfile)
</script>
